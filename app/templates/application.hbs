<!--header-->
<nav class="" style="top: 0px;position: sticky; background:white">
  <div class="layout-row layout-align-space-around-center">
    <div class="flex">
      <img alt="blog" height="30px" src="https://www.shijiezhou.com/images/newlogo.png"
        style="display:block;margin-left:auto;margin-right:auto;" width="30px">
    </div>
    <div class="flex-50"></div>
    <div class="flex">
      <i class="material-icons">search</i>
      <i class="material-icons">notification_important</i>
      <img src="https://cdn-images-1.medium.com/fit/c/64/64/0*b90Ej2bAcyCdYYuY" alt="user image" width="30px"
        height="30px">
    </div>
  </div>
  <div class="layout-row layout-align-center">
    {{#paper-button onClick=(action "HOME")}}HOME{{/paper-button}}
    {{#paper-button onClick=(action "SELF")}}SELF{{/paper-button}}
    {{#paper-button onClick=(action "TECH")}}TECH{{/paper-button}}
    {{#paper-button onClick=(action "HEATED")}}HEATED{{/paper-button}}
    {{#paper-button onClick=(action "DESIGN")}}DESIGN{{/paper-button}}
    {{#paper-button onClick=(action "CULTURE")}}CULTURE{{/paper-button}}
    {{#paper-button onClick=(action "MORE")}}MORE{{/paper-button}}
  </div>
</nav>

